<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProjectCompScience.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProjectCompScience"
    Shell.FlyoutBehavior="Disabled"
    Title="ProjectCompScience">

    <ShellContent
        Title="addNewSharePage"
        ContentTemplate="{DataTemplate View:addNewShares}"
        Route="addNewSharesPage" />

    <ShellContent
    Title="StockSharesPage"
    ContentTemplate="{DataTemplate View:StockSharesPage}"
    Route="StockSharesPage" />

</Shell>
