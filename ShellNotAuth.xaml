<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProjectCompScience.ShellNotAuth"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProjectCompScience"
    xmlns:View="clr-namespace:ProjectCompScience.View"
    Shell.FlyoutBehavior="Flyout"
    Title="ProjectCompScience"
    FlowDirection="LeftToRight"
    >

    <ShellContent
        Title="Register"
        ContentTemplate="{DataTemplate View:Register}"
        Route="Register" />
    
    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate View:Login}"
        Route="Login" />

</Shell>
